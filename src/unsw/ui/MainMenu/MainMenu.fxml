<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="root" prefHeight="599.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unsw.ui.MainMenu.MainMenuController">
   <children>
      <Pane HBox.hgrow="ALWAYS">
         <children>
            <ImageView fx:id="mainpicture" fitHeight="202.0" fitWidth="444.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <VBox fx:id="menu" alignment="CENTER" prefHeight="400.0" prefWidth="203.0">
         <children>
            <Pane VBox.vgrow="ALWAYS">
               <children>
                  <ImageView fx:id="sidepicture" fitHeight="226.0" fitWidth="155.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </Pane>
            <Button fx:id="newGame" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#newGame" text="New Game">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Local Network">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Online Game">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#loadsave" text="Load Game">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="Setting" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#setting" text="Setting">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="quit" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#quit" text="Quit">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</HBox>
